<p id="notice"><%= notice %></p>

<h1>Listing Service To Types</h1>

<table>
  <thead>
    <tr>
      <th>Service provider</th>
      <th>Service type</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @service_to_types.each do |service_to_type| %>
      <tr>
        <td><%= service_to_type.service_provider %></td>
        <td><%= service_to_type.service_type %></td>
        <td><%= link_to 'Show', service_to_type %></td>
        <td><%= link_to 'Edit', edit_service_to_type_path(service_to_type) %></td>
        <td><%= link_to 'Destroy', service_to_type, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Service to type', new_service_to_type_path %>
